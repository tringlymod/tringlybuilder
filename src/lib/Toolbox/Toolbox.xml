    <block type="menus_items" />
    <sep gap="48" />
    <block type="menus_createstatic" />
    <block type="menus_createdynamic" />
    </category>
    <category name="Functions" colour="#5531D6">
        <block type="functions_create">
            <statement name="FUNC">
                <block type="functions_return" />
            </statement>
        </block>
        <block type="functions_inline">
            <statement name="FUNC">
                <block type="functions_return" />
            </statement>
        </block>
        <sep gap="48" />
        <block type="functions_return" />
        <sep gap="48" />
        <block type="functions_call" />
        <block type="functions_callreporter" />
    </category>
    <category name="Technical" colour="#666666">
        <block type="debug_log" />
        <block type="debug_warn" />
        <block type="debug_error" />
        <sep gap="48" />
        <block type="debug_group" />
        <block type="debug_groupcollapsed" />
        <sep gap="48" />
        <block type="debug_evalblock" />
        <block type="debug_rawblock" />
        <block type="debug_eval" />
        <block type="debug_raw" />
        <sep gap="48" />
        <block type="debug_comment" />
        <block type="debug_commentstack" />
        <sep gap="48" />
        <block type="debug_catch" />
    </category>
    <sep></sep>
    <category name="Scratch VM" colour="#FF8F05">
        <block type="ScratchVM_mousedown" />
        <block type="ScratchVM_username" />
        <block type="ScratchVM_loggedin" />
